<% layout('../template/layout') -%>

<section class="wrapper site-min-height">
		    
	<div class="row">
		<div class="col-lg-10">
          	<h3><i class="fa fa-angle-right"></i> 사진</h3>
        </div>
        <div class="col-lg-2">
        	<a href="/photo/photowrite"><button type="button" class="btn btn-danger" style="margin-top:20px;">사진 추가하기</button></a>
        </div>
    </div>
    <hr>
    <div class="row mt">
     	<% photoTable.forEach(function(photoTable){ %>
     		<div class="col-lg-3 col-md-4 col-sm-4 col-xs-12 desc">
				<div class="project-wrapper">
		        	<div class="project">
	                	<div class="col-lg-11 col-xs-2">
		            		<div class="photo-wrapper" style="background-color:transparent; width:220px;">
		                		<div class="thumbnail photo" rel="lightbox" style="width:200px; height:230px;">
		                    		<a class="fancybox" type="image" href="/uploads/photo/<%=photoTable.photoAdd%>">
		                        		<img style="width:220px; height:200px" class="img-responsive" src="uploads/photo/<%=photoTable.photoAdd%>" alt="">
		                        	<div class='text-right'>
                        			<small class='text-muted'><%=photoTable.title%></small>
                    			</div>
		                        	</a>
		                       	</div>
		                       	<br>
		                    	
                    		</div>	
                    		
		                  	<div class="overlay">
		                  	</div>
		                </div>
		            </div>
		        </div>
			</div>
			<% }); %>
	</div>
</section>

