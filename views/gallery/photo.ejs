<% layout('../template/layout') -%>

<section class="wrapper site-min-height">
		    
	<div class="row">
		<div class="col-lg-10">
          	<h3><i class="fa fa-angle-right"></i> 사진</h3>
        </div>
        <div class="col-lg-2">
        	<a href="/photo/photowrite"><button type="button" class="btn btn-danger" style="margin-top:20px;">사진 추가하기</button></a>
        </div>
    </div>
    <hr>
    <div class="row mt">
     	<% photoTable.forEach(function(photoTable){ %>
     		<div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 desc">
				<div class="project-wrapper">
		        	<div class="project">
		            	<div class="photo-wrapper" style="background-color:transparent; min-height:200px; max-height:200px; width:100%; margin-bottom:40px;" >
		                	<div class="photo">
		                    	<a class="fancybox" type="image" href="uploads/photo/<%=photoTable.photoAdd%>">
		                        <img style="width=100%;" class="img-responsive" src="uploads/photo/<%=photoTable.photoAdd%>" alt=""></a>
		                    </div>
		                  	<div class="overlay">
		                  	</div>
		                </div>
		            </div>
		        </div>
			</div>
			<% }); %>
	</div>
</section>

